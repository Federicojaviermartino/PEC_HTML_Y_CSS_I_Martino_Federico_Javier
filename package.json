    "@babel/preset-env": "^7.26.0",
    "@parcel/transformer-sass": "^2.12.0",
    "autoprefixer": "^10.4.20",
    "parcel": "^2.12.0",
    "postcss": "^8.4.49",
    "rimraf": "^6.1.2",
    "sharp": "^0.34.5"
  },
  "dependencies": {
    "leaflet": "^1.9.4"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ]
